<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="person.nicholas.crm.controller.CustomerTabController">
    <TableView fx:id="customerTableView" layoutX="14.0" layoutY="48.0" prefHeight="405.0" prefWidth="487.0">
        <columns>
            <TableColumn fx:id="customerId" prefWidth="103.0" text="Customer ID" />
            <TableColumn fx:id="customerName" prefWidth="103.0" text="Customer Name" />
            <TableColumn fx:id="contactNum" prefWidth="125.0" text="Contact Number" />
        </columns>
    </TableView>
    <Button fx:id="deleteCustomer" layoutX="448.0" layoutY="15.0" mnemonicParsing="false" onAction="#deleteCustomer" text="Delete" />
    <Button fx:id="searchCustomer" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#searchCustomer" text="Search" />
    <Button fx:id="addCustomer" layoutX="388.0" layoutY="15.0" mnemonicParsing="false" onAction="#addCustomer" text="Add" />
    <TextField fx:id="searchCustomerField" layoutX="81.0" layoutY="15.0" />
</AnchorPane>
