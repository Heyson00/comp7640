<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="person.nicholas.crm.controller.VendorTabController" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <TableView fx:id="vendorView" layoutX="14.0" layoutY="48.0" prefHeight="405.0" prefWidth="484.0">
            <columns>
                <TableColumn fx:id="vendorId" prefWidth="75.0" text="Vendor ID" />
                <TableColumn fx:id="businessName" prefWidth="125.0" text="Name" />
                <TableColumn fx:id="feedbackScore" prefWidth="59.0" text="Score" />
                <TableColumn fx:id="geographicalPresence" prefWidth="135.0" text="Geographical Presence" />
            </columns>
        </TableView>
        <Button fx:id="addVendor" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onAddButtonClick" text="Add" />
</AnchorPane>

